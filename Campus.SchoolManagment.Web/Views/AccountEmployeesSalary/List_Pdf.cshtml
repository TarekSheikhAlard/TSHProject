            <td>
                @Html.DisplayFor(modelItem => item.Dedecation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalDedecated)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NetSalary)
            </td>

        </tr>
    }

